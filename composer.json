    "require": {
        "hiqdev/yii2-asset-pnotify": "*",
        "yiisoft/yii2-bootstrap": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2": "dev-master",
        "yiisoft/yii2-bootstrap": "dev-master",
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\pnotify\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "web": "src/config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
